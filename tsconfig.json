  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist/",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
